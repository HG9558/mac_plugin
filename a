const openRequestLog = (row: OperationLogDataAPI.OperationLogListFetchList) => {
  addDialog({
    title: t('请求参数'),
    props: {
      requestLog: JSON.parse(row.requestLog) ?? {}
    },
    width: '40%',
    draggable: true,
    hideFooter: true,
    fullscreenIcon: true,
    closeOnClickModal: false,
    contentRenderer: () => h(RequestDialog)
  });
};

//- 组件RequestDialog

<template>
  <vue-json-pretty
    v-model:data="renderData"
    :deep="3"
    :show-double-quotes="true"
    :show-line="true"
    :show-length="true"
    :show-line-number="true"
    :editable="false"
  />
</template>

<script setup lang="ts">
import VueJsonPretty from 'vue-json-pretty';
import 'vue-json-pretty/lib/styles.css';
import { JSONDataType } from 'vue-json-pretty/types/utils';
const props = defineProps<{
  requestLog: JSONDataType;
}>();

const renderData = reactive<JSONDataType>(props.requestLog);
</script>

<style scoped></style>

ts报错 

类型“JSONDataType”不满足约束“object”。
  不能将类型“string”分配给类型“object”。ts(2344)
(alias) type JSONDataType = string | number | boolean | unknown[] | Record<string, unknown>
import JSONDataType
