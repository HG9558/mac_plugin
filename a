  <template #eventTime="{ row, index }: MatchEventRowType">
                    <MatchTime :row="row" :index="index" :item="item" />
                  </template>

//- 组件

<template>
  <el-time-picker
    v-model="row.eventTime"
    :placeholder="t('请选择时间')"
    :disabled="!row.isEdit"
    :default-value="new Date(new Date().toLocaleDateString())"
    value-format="x"
    @change="changeEventTime(row, index, item.tableList)"
    format="HH:mm:ss"
  />
</template>

<script setup lang="ts">
import { t } from '@/plugins/i18n';
import { MatchEventRowType } from '../utils/type';

defineProps<MatchEventRowType>();
const emits = defineEmits(['changeEventTime']);

const changeEventTime = (...data) => emits('changeEventTime', ...data);
</script>

<style scoped></style>


export type MatchEventRowType = {
  row: SattleDataAPI.MatchEventsList;
  index?: number;
}
